<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="seq_type_min" model="ir.sequence.type">
            <field name="name">MIN Code</field>
            <field name="code">min.code</field>
        </record>

        <record id="min_seq" model="ir.sequence">
            <field name="name">MIN Code</field>
            <field name="code">min.code</field>
            <field name="prefix">MIN/</field>
             <field name="implementation">no_gap</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_type_mrn" model="ir.sequence.type">
            <field name="name">MRN Code</field>
            <field name="code">mrn.code</field>
        </record>

        <record id="mrn_seq" model="ir.sequence">
            <field name="name">MRN Code</field>
            <field name="code">mrn.code</field>
            <field name="prefix">MRN/</field>
            <field name="padding">3</field>
             <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_type_gtn" model="ir.sequence.type">
            <field name="name">GTN Code</field>
            <field name="code">gtn.code</field>
        </record>

        <record id="gtn_seq" model="ir.sequence">
            <field name="name">GTN Code</field>
            <field name="code">gtn.code</field>
            <field name="prefix">MTN/</field>
             <field name="implementation">no_gap</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    	
        <record id="seq_type_grr" model="ir.sequence.type">
            <field name="name">GRR Code</field>
            <field name="code">grr.code</field>
        </record>

        <record id="grr_seq" model="ir.sequence">
            <field name="name">GRR Code</field>
            <field name="code">grr.code</field>
            <field name="prefix">GRN/</field>
            <field name="padding">3</field>
              <field name="implementation">no_gap</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_type_site_purchase" model="ir.sequence.type">
            <field name="name">Site Purchase</field>
            <field name="code">site.purchase</field>
        </record>
 <record id="sequence_site_purchase" model="ir.sequence">
            <field name="name">Site Purchase</field>
            <field name="code">site.purchase</field>
            <field name="padding">4</field>
            <field name="prefix"></field>
            <field name="suffix">/%(year)s</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_type_driver_statement" model="ir.sequence.type">
            <field name="name">Driver Statement</field>
            <field name="code">driver.daily.statement</field>
        </record>
 <record id="sequence_driver_statement" model="ir.sequence">
            <field name="name">Driver Statement</field>
            <field name="code">driver.daily.statement</field>
            <field name="padding">4</field>
            <field name="prefix">DRIVER-STMT/</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>


        
        
    </data>
</openerp>




       

   
